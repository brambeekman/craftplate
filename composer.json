{
  "name": "brambeekman/craftplate",
  "description": "Standard Craft install",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Bram Beekman",
      "email": "hello@brambeekman.com",
      "homepage": "https://brambeekman.com"
    }
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "require": {
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/cms": "^3.3.7",
    "verbb/super-table": "^2.3.0",
    "craftcms/redactor": "^2.4.0",
    "nystudio107/craft-seomatic": "3.3.40",
    "nystudio107/craft-twigpack": "^1.2.9"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
